{
  "name": "owokx",
  "version": "0.3.0",
  "description": "Cloudflare Workers MCP server for autonomous stock trading with Alpaca",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "deploy:oneclick": "npx tsx scripts/deploy.ts",
    "benchmark:swarm": "node scripts/benchmark-swarm.mjs",
    "db:migrate": "wrangler d1 migrations apply Okx-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply Okx-db --remote",
    "db:studio": "wrangler d1 execute Okx-db --local --command 'SELECT 1'",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format --write src/",
    "format:check": "biome format src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "secrets:scan": "node scripts/scan-secrets.mjs",
    "hooks:install": "node scripts/setup-git-hooks.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "node scripts/ci-test-runner.mjs",
    "setup:access": "npx tsx scripts/setup-access.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "@ai-sdk/cohere": "^3.0.21",
    "@ai-sdk/deepseek": "^2.0.20",
    "@ai-sdk/google": "^3.0.29",
    "@ai-sdk/groq": "^3.0.24",
    "@ai-sdk/mistral": "^3.0.20",
    "@ai-sdk/openai": "^3.0.29",
    "@ai-sdk/xai": "^3.0.57",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "agents": "^0.0.74",
    "ai": "^6.0.86",
    "okx-api": "^3.1.2",
    "openai": "^6.22.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.0",
    "@cloudflare/workers-types": "^4.20260214.0",
    "@types/node": "^22.19.11",
    "@types/ws": "^8.18.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9",
    "wrangler": "^4.65.0",
    "ws": "^8.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
